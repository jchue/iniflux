{
  "name": "miniflux-client",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -n \"server,client\" -c \"magenta,blue\" \"npm run server-dev\" \"npm run client-dev\"",
    "client-dev": "cd client && npm run start",
    "server-dev": "nodemon ./bin/www",
    "build": "cd client && npm run build && mv build ..",
    "start": "node ./bin/www"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/node": "^7.14.9",
    "@babel/preset-env": "^7.15.0",
    "concurrently": "^6.2.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "nodemon": "^2.0.12"
  }
}
